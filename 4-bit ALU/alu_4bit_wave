:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56606945c490 .scope module, "alu_4bit_tb" "alu_4bit_tb" 2 3;
 .timescale -9 -10;
v0x5660694ac7f0_0 .var "a", 3 0;
v0x5660694ac8d0_0 .var "b", 3 0;
v0x5660694ac9a0_0 .net "result", 4 0, v0x5660694ac570_0;  1 drivers
v0x5660694acaa0_0 .var "sel", 1 0;
S_0x56606945c620 .scope module, "dut" "alu_4bit" 2 9, 3 1 0, S_0x56606945c490;
 .timescale -9 -10;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 4 "b";
    .port_info 2 /INPUT 2 "sel";
    .port_info 3 /OUTPUT 5 "result";
v0x566069498ba0_0 .net "a", 3 0, v0x5660694ac7f0_0;  1 drivers
v0x5660694ac490_0 .net "b", 3 0, v0x5660694ac8d0_0;  1 drivers
v0x5660694ac570_0 .var "result", 4 0;
v0x5660694ac660_0 .net "sel", 1 0, v0x5660694acaa0_0;  1 drivers
E_0x5660694993a0 .event anyedge, v0x5660694ac660_0, v0x566069498ba0_0, v0x5660694ac490_0;
    .scope S_0x56606945c620;
T_0 ;
    %wait E_0x5660694993a0;
    %load/vec4 v0x5660694ac660_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x5660694ac570_0, 0, 5;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x566069498ba0_0;
    %pad/u 5;
    %load/vec4 v0x5660694ac490_0;
    %pad/u 5;
    %and;
    %store/vec4 v0x5660694ac570_0, 0, 5;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x566069498ba0_0;
    %pad/u 5;
    %load/vec4 v0x5660694ac490_0;
    %pad/u 5;
    %or;
    %store/vec4 v0x5660694ac570_0, 0, 5;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x566069498ba0_0;
    %pad/u 5;
    %load/vec4 v0x5660694ac490_0;
    %pad/u 5;
    %add;
    %store/vec4 v0x5660694ac570_0, 0, 5;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x566069498ba0_0;
    %pad/u 5;
    %load/vec4 v0x5660694ac490_0;
    %pad/u 5;
    %sub;
    %store/vec4 v0x5660694ac570_0, 0, 5;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x56606945c490;
T_1 ;
    %vpi_call 2 20 "$dumpfile", "test_alu_4bit.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x56606945c490 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5660694acaa0_0, 0, 2;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x5660694ac7f0_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5660694ac8d0_0, 0, 4;
    %delay 50, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x5660694ac7f0_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5660694ac8d0_0, 0, 4;
    %delay 50, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5660694acaa0_0, 0, 2;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x5660694ac7f0_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5660694ac8d0_0, 0, 4;
    %delay 50, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x5660694ac7f0_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5660694ac8d0_0, 0, 4;
    %delay 50, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5660694acaa0_0, 0, 2;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5660694ac7f0_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x5660694ac8d0_0, 0, 4;
    %delay 50, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x5660694ac7f0_0, 0, 4;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5660694ac8d0_0, 0, 4;
    %delay 50, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5660694acaa0_0, 0, 2;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5660694ac7f0_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5660694ac8d0_0, 0, 4;
